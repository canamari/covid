<p id="notice"><%= notice %></p>

<h1>Cadastres</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Cpf</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cadastres.each do |cadastre| %>
      <tr>
        <td><%= cadastre.name %></td>
        <td><%= cadastre.cpf %></td>
        <td><%= cadastre.phone %></td>
        <td><%= link_to 'Show', cadastre %></td>
        <td><%= link_to 'Edit', edit_cadastre_path(cadastre) %></td>
        <td><%= link_to 'Destroy', cadastre, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cadastre', new_cadastre_path %>
